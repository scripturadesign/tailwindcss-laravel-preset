{
    "name": "scriptura/tailwindcss-preset",
    "description": "Laravel 5.5.x Front-end preset for tailwindcss",
    "keywords": ["laravel", "preset", "tailwind", "tailwindcss"],
    "license": "MIT",
    "require": {
        "laravel/framework": "5.5.*|5.6.*"
    },
    "autoload": {
        "psr-4": {
            "Scriptura\\TailwindcssPreset\\": "src/"
        }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Scriptura\\TailwindcssPreset\\TailwindcssPresetServiceProvider"
       ]
      }
    }

}
